{
    "routes": [
        {
            "route": "/",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/login",
            "rewrite": "/.auth/login/aad"
        },
        {
            "route": "/logout",
            "rewrite": "/.auth/logout"
        }
    ],
    "responseOverrides": {
        "401": {
            "redirect": "/login"
        }
    },"auth": {
        "identityProviders": {
            "azureActiveDirectory": {
                "registration": {
                "openIdIssuer": "https://login.microsoftonline.com/dd82b190-6d71-47ce-89c4-b63cec673972/v2.0",
                "clientIdSettingName": "AZURE_CLIENT_ID",
                "clientSecretSettingName": "AZURE_CLIENT_SECRET_APP_SETTING_NAME"
                }
            }
        }
    }
}