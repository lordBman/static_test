{
    "routes": [
        {
            "route": "/",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/login",
            "redirect": "/.auth/login/github"
        },
        {
            "route": "/logout",
            "redirect": "/.auth/logout"
        }
    ],
    "responseOverrides": {
        "401": {
            "rewrite": "/.auth/login/github",
            "statusCode": 200
        }
    }
}